<script type="text/javascript" src="/media/javascript/jquery.min.js"></script>
<script type="text/javascript" src="/media/javascript/jquery-table-sorter.js"></script>
<script type="text/javascript">
var boldSorter = function (node) {
  console.log(node);
  node = node.childNodes[0];
  if (node.textContent.match(/unused|none/))
    return "~"; // ASCII sorting
  return node.textContent;
};

jQuery(function () {
  jQuery("table").tablesorter({
    cssHeader: "",
    textExtraction: boldSorter
    /* headers: {
      3: {
        sorter: false
      }
    } */
  });
});
</script>

